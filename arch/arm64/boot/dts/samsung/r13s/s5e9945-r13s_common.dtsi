#define BOARD_ID	0x0

/ {
	compatible = "samsung,armv9", "samsung,s5e9945";
	board_id = <BOARD_ID>;
	board_rev = <0x0>;

	fragment@model {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;

			model = "Samsung S5E9945 board based on ERD9945";

			gpio_keys {
				status = "okay";
				compatible = "gpio-keys";
				pinctrl-names = "default";
				pinctrl-0 = <&key_volup>;

				button@3 {
					label = "gpio-keys: KEY_VOLUMEUP";
					linux,code = <115>;
					gpios = <&gpa3 0 0xf>;
					debounce-interval = <0x0f>;
					gpio-key,wakeup;
				};
			};

			vbat: vbat {
				compatible = "regulator-fixed";
				regulator-name = "VBAT";
				regulator-min-microvolt = <4200000>;
				regulator-max-microvolt = <4200000>;
				regulator-boot-on;
			};

			dummy_audio_codec: audio-codec-dummy {
				compatible = "snd-soc-dummy";
				#sound-dai-cells = <0>;
			};

			dummy_audio_cpu: audio-cpu-dummy {
				compatible = "samsung,dummy-cpu";
				#sound-dai-cells = <0>;
			};

			abox_synchronized_ipc_aw: abox_synchronized_ipc_aw {
				compatible = "samsung,abox-synchronized-ipc-aw";
				#sound-dai-cells = <1>;
				abox = <&abox>;
				status = "okay";
			};

			sound {
				compatible = "samsung,exynos9945-audio";

				MICBIAS1-supply = <&l12_reg>;
				MICBIAS2-supply = <&l12_reg>;

				/* Trick for probe order */
				dmas = <&dp_dma0>;

				samsung,routing =
					"DMIC1", "MICBIAS1",
					"DMIC2", "MICBIAS2",
					"BLUETOOTH SPK", "ABOX UAIF2 Playback",
					"ABOX UAIF2 Capture", "BLUETOOTH MIC",
					"USB SPK", "ABOX USB Playback",
					"ABOX USB Capture", "USB MIC",
					"FWD SPK", "ABOX FWD Playback",
					"ABOX FWD Capture", "FWD MIC",
					"VTS Virtual Output", "VTS Virtual Output Mux",
					"PAD DPDM", "DMIC1",
					"PAD DPDM", "DMIC2",
					"PAD DPDM", "DMIC3";

